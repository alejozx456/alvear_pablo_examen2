        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyectoFactura">
        <activity android:name=".LoginPAAV"></activity>
        <activity android:name=".MainActivityDetalleProductoPAAV" />
        <activity android:name=".MainActivityPAAV">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>